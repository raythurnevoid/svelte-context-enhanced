{
	"name": "website",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"type": "module",
	"imports": {
		"#src/*": "./src/*",
		"#src/*.js": "./src/*"
	},
	"devDependencies": {
		"@originjs/vite-plugin-commonjs": "1.0.3",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.4.1",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2",
		"sass": "~1.35.1",
		"vite-tsconfig-paths": "^3.4.1"
	},
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"@lukeed/uuid": "^2.0.0",
		"@raythurnevoid/svelte-context-enhanced": "^1.0.1",
		"@raythurnevoid/svelte-group-components": "^0.0.29",
		"@raythurnevoid/svelte-hooks": "^0.0.10",
		"@svelte-material-ui-test/core": "^2.0.0-alpha.12",
		"cookie": "^0.4.1",
		"svelte": "^3.46.2",
		"svelte-prism": "https://github.com/raythurnevoid/svelte-prism/releases/download/typescript/svelte-prism-1.0.0.tgz"
	}
}
