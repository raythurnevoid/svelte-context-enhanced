{
	"name": "website",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"prepare": "npm run smui-theme",
		"smui-theme": "smui-theme compile src/styles/smui/smui.css --output-style=expanded --includes src/styles/smui",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"type": "module",
	"imports": {
		"#src/*": "./src/*",
		"#src/*.js": "./src/*"
	},
	"devDependencies": {
		"@material-icons/font": "^1.0.34",
		"@material/typography": "^13.0.0",
		"@originjs/vite-plugin-commonjs": "1.0.3",
		"@smui/button": "^6.1.0",
		"@smui/common": "^6.1.0",
		"@smui/drawer": "^6.1.0",
		"@smui/icon-button": "^6.1.0",
		"@smui/list": "^6.1.0",
		"@smui/top-app-bar": "^6.1.0",
		"@sveltejs/adapter-static": "1.0.0-next.39",
		"@sveltejs/kit": "1.0.0-next.450",
		"@types/cookie": "^0.5.1",
		"autoprefixer": "^10.4.8",
		"copyfiles": "^2.4.1",
		"cssnano": "^5.1.13",
		"cssnano-preset-advanced": "^5.3.8",
		"del-cli": "^5.0.0",
		"postcss": "^8.4.16",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.54.6",
		"smui-theme": "^6.1.3",
		"svelte": "^3.49.0",
		"svelte-check": "^2.9.0",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.0",
		"typescript": "4.8.2"
	},
	"dependencies": {
		"@fontsource/material-icons": "^4.5.4",
		"@fontsource/roboto": "^4.5.8",
		"@fontsource/roboto-mono": "^4.5.8",
		"@lukeed/uuid": "^2.0.0",
		"@raythurnevoid/svelte-context-enhanced": "^1.0.2",
		"@raythurnevoid/svelte-group-components": "^0.0.29",
		"@raythurnevoid/svelte-hooks": "^0.0.10",
		"cookie": "^0.5.0",
		"svelte-prism": "https://github.com/raythurnevoid/svelte-prism/releases/download/typescript/svelte-prism-1.0.0.tgz"
	}
}
