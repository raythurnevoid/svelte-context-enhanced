{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"#src/*": [
				"src/*"
			],
			"$lib": [
				"src/lib"
			],
			"$lib/*": [
				"src/lib/*"
			],
			"@svelte-material-design/core/*": [
				"node_modules/@svelte-material-ui-test/core/packages/*",
			],
			"@svelte-material-ui-test/core/*": [
				"node_modules/@svelte-material-ui-test/core/packages/*",
			]
		}
	},
}