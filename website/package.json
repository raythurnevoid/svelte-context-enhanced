{
	"name": "website",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync && npm run smui-theme",
		"smui-theme": "smui-theme compile src/styles/smui.css -i src/styles",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"type": "module",
	"imports": {
		"#src/*": "./src/*",
		"#src/*.js": "./src/*"
	},
	"devDependencies": {
		"@fontsource/material-icons": "^4.5.4",
		"@fontsource/roboto": "^4.5.5",
		"@fontsource/roboto-mono": "^4.5.7",
		"@material-icons/font": "^1.0.27",
		"@material/typography": "^13.0.0",
		"@originjs/vite-plugin-commonjs": "1.0.3",
		"@smui/button": "^6.0.0-beta.16",
		"@smui/common": "^6.0.0-beta.16",
		"@smui/drawer": "^6.0.0-beta.16",
		"@smui/icon-button": "^6.0.0-beta.16",
		"@smui/list": "^6.0.0-beta.16",
		"@smui/top-app-bar": "^6.0.0-beta.16",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.5.1",
		"autoprefixer": "^10.4.4",
		"copyfiles": "^2.4.1",
		"cssnano": "^5.1.7",
		"cssnano-preset-advanced": "^5.3.3",
		"del-cli": "^4.0.1",
		"postcss": "^8.4.12",
		"postcss-load-config": "^3.1.4",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.49.11",
		"smui-theme": "^6.0.0-beta.16",
		"svelte": "^3.47.0",
		"svelte-check": "^2.7.0",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "4.7.0-beta",
		"vite-tsconfig-paths": "^3.4.1"
	},
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.7",
		"@lukeed/uuid": "^2.0.0",
		"@raythurnevoid/svelte-context-enhanced": "^1.0.2",
		"@raythurnevoid/svelte-group-components": "^0.0.29",
		"@raythurnevoid/svelte-hooks": "^0.0.10",
		"cookie": "^0.5.0",
		"svelte-prism": "https://github.com/raythurnevoid/svelte-prism/releases/download/typescript/svelte-prism-1.0.0.tgz"
	}
}
